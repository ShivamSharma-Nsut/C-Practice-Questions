#include <stdio.h>

int main()
{
   int m , n , i , j ,a , b , c , d ;
   int arr1[m][n] , arr2[m][n] , matSum[m][n] ;
   printf("Enter First Matrix");
   for (i=0 ; i<m ; i++){

        for (j=0 ; j<n ; j++)
            scanf("%d",&arr1[i][j]);
            printf("\n");
   }

   printf("Enter Second Matrix");
   for (a=0 ; a<m ; a++){

        for (b=0 ; b<n ; b++)
            scanf("%d",&arr2[a][b]);
            printf("\n");
   }
   for (c=0 ; c<m ; c++){

        for (d=0 ; d<n ; d++)
            matSum[c][d] = arr1[c][d] + arr2[c][d] ;
            printf("%d",&matSum[c][d]);
            printf("\n");
   }

    return 0;
}
